{
  "name": "webpack4course",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config configs/webpack.config.js",
    "webpack:dev": "npm run webpack -- --env.mode development",
    "webpack:watch": "npm run webpack -- --env.mode development --watch",
    "webpack:prod": "npm run webpack -- --env.mode production",
    "webpack:inspect": "node --inspect --inspect-brk ./node_modules/.bin/webpack --config configs/webpack.config.js --env.mode production",
    "stats": "webpack --config configs/webpack.config.js --env.mode production --progress --profile --json > stats.json",
    "wba": "webpack-bundle-analyzer stats.json",
    "sme": "source-map-explorer dist/*.js",
    "bundlestats": "bundle-stats --baseline stats.json",
    "start": "webpack-dev-server --config configs/webpack.config.js --env.mode development --open",
    "start:dashboard": "webpack-dashboard -- webpack-dev-server --config configs/webpack.config.js --env.mode development --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pushkar100/webpack4course.git"
